#!/usr/bin/env bash
#this script uses case to display text on different iterations
num=1
while (($num <= 10))
do
	case $num in
	     4)
		echo "bad luck from China"
		;;
	     9)
		echo "bad luck from Japan"
		;;
	     17)
		echo "bad luck from Italy"
		;;
	     *)
		echo $num
	esac
	((num ++))
done
